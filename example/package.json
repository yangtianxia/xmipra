{
  "name": "example",
  "description": "xmipra example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "weapp-tw patch",
    "build:weapp": "taro build --type weapp",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "weapp": "npm run build:weapp -- --watch",
    "alipay": "npm run build:alipay -- --watch",
    "tt": "npm run build:tt -- --watch",
    "lint": "eslint ./src --ext .vue,.ts,.tsx --fix --ignore-path .gitignore",
    "format": "prettier ./src --write"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,md}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{less,css}": [
      "stylelint --fix",
      "prettier --write"
    ]
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8",
    "not ie > 0",
    "not ie_mob > 0"
  ],
  "author": "yangtianxia <1304082170@qq.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yangtianxia/xmiprae.git"
  },
  "bugs": {
    "url": "https://github.com/yangtianxia/xmipra/issues"
  },
  "homepage": "https://github.com/yangtianxia/xmipra#readme",
