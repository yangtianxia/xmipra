{
  "name": "xmipra",
  "version": "0.0.0",
  "description": "Miniprogram development API.",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.mjs",
      "require": "./dist/index.cjs.js"
    }
  },
  "entry": [
    {
      "root": true,
      "name": "index",
      "filepath": "index.ts"
    }
  ],
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "lint": "eslint ./src --ext .ts,.tsx --fix",
    "format": "prettier ./src --write",
    "build:types": "tsc -p ./tsconfig.json --emitDeclarationOnly",
    "build:bundle": "ts-node ./build.ts",
    "build": "npm run clean && npm run lint && npm run build:types && npm run build:bundle",
    "dev": "npm run build -- --w"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "xmipra",
    "tarojs",
    "vue3"
  ],
  "license": "MIT",
  "author": "yangtianxia <1304082170@qq.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yangtianxia/xmipra.git",
    "directory": "packages/xmipra"
  },
  "bugs": {
    "url": "https://github.com/yangtianxia/xmipra/issues"
  },
  "homepage": "https://github.com/yangtianxia/xmipra#readme",
  "peerDependencies": {
    "@tarojs/components": "4.0.13",
    "@tarojs/taro": "4.0.13",
    "@txjs/bool": "^1.0.5",
    "@txjs/shared": "^1.0.7",
    "@txjs/types": "^1.1.0",
    "@types/extend": "^3.0.4",
    "debounce": "^2.2.0",
    "extend": "^3.0.2",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@douyin-microapp/typings": "^1.3.1",
    "@mini-types/alipay": "^3.0.14",
    "@tarojs/components": "4.0.13",
    "@tarojs/taro": "4.0.13",
    "@txjs/bool": "^1.0.5",
    "@txjs/shared": "^1.0.7",
    "@txjs/types": "^1.1.0",
    "@types/extend": "^3.0.4",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "debounce": "^2.2.0",
    "eslint-config-taro": "4.0.13",
    "eslint-plugin-vue": "^8.7.1",
    "extend": "^3.0.2",
    "glob": "^11.0.2",
    "miniprogram-api-typings": "^4.0.7",
    "vue": "^3.5.13"
  }
}
